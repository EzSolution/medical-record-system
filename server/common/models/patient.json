{
  "name": "Patient",
  "plural": "Patients",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "patientId": {
      "type": "string",
      "id": true,
      "required": true,
      "defaultFn": "uuidv4",
      "mongodb": {
        "columnName": "_id"
      }
    },
    "name": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "number",
      "required": true
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "civilStatus": {
      "type": "string",
      "required": true
    },
    "occupation": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "birthDate": {
      "type": "date",
      "required": true
    },
    "refferedBy": {
      "type": "string"
    },
    "contactNumber": {
      "type": "string",
      "required": true
    },
    "dateRegistered": {
      "type": "date",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "consultations": {
      "type": "hasMany",
      "model": "Consultation",
      "foreignKey": "patientId"
    }
  },
  "acls": [],
  "methods": {}
}
